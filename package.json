{
  "name": "@yimoko/antd",
  "version": "0.1.10",
  "author": "ickeep <i@ickeep.com>",
  "private": false,
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ickeep/yimoko-antd.git"
  },
  "exports": {
    ".": {
      "import": "./dist/yimoko-antd.es.js",
      "require": "./dist/yimoko-antd.cjs.js",
      "default": "./src/library.ts"
    }
  },
  "main": "src/library.ts",
  "types": "types/library.d.ts",
  "files": [
    "src",
    "dist",
    "types",
    "README.md"
  ],
  "dependencies": {
    "@formily/core": "^2.3.1",
    "@formily/react": "^2.3.1",
    "@formily/reactive": "^2.3.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.67",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@yimoko/store": "^2.0.16",
    "antd": "^5.12.1",
    "dayjs": "^1.11.10",
    "htmr": "^1.0.2",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "lint": "eslint ./src --fix --ext ts,tsx",
    "build": "craco build",
    "library": "vite build",
    "start": "craco start",
    "test:unit": "craco test  --coverage --watchAll=false",
    "test:watch": "craco test --coverage --watchAll"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/index.tsx",
      "reportWebVitals",
      "/src/_demo"
    ],
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs",
      "@yimoko/store": "@yimoko/store/dist/yimoko-store.cjs",
      "^lodash-es$": "lodash"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@ant-design/icons": "^5.2.6",
    "@craco/craco": "^7.1.0",
    "@types/lodash-es": "^4.17.12",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint-config-tencent": "^1.0.4",
    "rc-picker": "^3.14.6",
    "vite": "^5.0.6",
    "vite-plugin-dts": "^3.6.4"
  }
}